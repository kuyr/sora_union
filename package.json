{
  "name": "sora_union",
  "version": "1.0.1",
  "description": "a test automation framework for testing web (it can be modified to cater to for mobile)",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "wdio": "wdio run ./wdio.conf.js",
    "login_test": "npx wdio wdio.conf.js --spec ./test/specs/login.spec.js",
    "appointments_test": "npx wdio wdio.conf.js --spec ./test/specs/appointment.spec.js",
    "report_generate":"npx allure generate allure-results --clean && npx allure open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuyr/webdriver.io_web_auto.git"
  },
  "author": "emmanuel kuye",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kuyr/webdriver.io_web_auto/issues"
  },
  "homepage": "https://github.com/kuyr/webdriver.io_web_auto#readme",
  "dependencies": {
    "@wdio/cli": "^8.22.1",
    "selenium-webdriver": "^4.15.0"
  },
  "devDependencies": {
    "@wdio/allure-reporter": "^8.21.0",
    "@wdio/local-runner": "^8.22.1",
    "@wdio/mocha-framework": "^8.22.0",
    "@wdio/spec-reporter": "^8.21.0",
    "webdriverio": "^8.22.1"
  }
}
